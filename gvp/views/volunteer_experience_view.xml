<?xml version='1.0' encoding='utf-8'?>
<odoo>


<!-- volunteer experience form view -->

  <record id="volunteer_experience_form_view" model="ir.ui.view">
    <field name="name">volunteer experience form view</field>
    <field name="model">volenteer.experience</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
            <group>
                <field name="name"/>
            </group>
            <group>
                <group>
                   <field name="role"/>
                   <field name="is_currently"/>
                </group>
                <group>

                    <field name="start_date"/>
                    <field name="end_date"/>

                </group>
            </group>
        </sheet>
      </form>
    </field>
  </record>

 <!-- education Tree view -->
  <record id="volunteer_experience_tree_view" model="ir.ui.view">
    <field name="name">Volunteer experience Tree view</field>
    <field name="model">volenteer.experience</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name"/>
            <field name="role"/>
            <field name="is_currently"/>
            <field name="start_date"/>
            <field name="end_date"/>

        </tree>
    </field>
  </record>

</odoo>