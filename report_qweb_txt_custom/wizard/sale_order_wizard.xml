<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_pos_details_wizard" model="ir.ui.view">
            <field name="name">Sale Details Wizard Form</field>
            <field name="model">sale.details.wizard</field>
            <field name="arch" type="xml">
                <form string="Sales Details">
                    <group>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>

                    <group>
                        <field name="partner_id"  string="Vendor"/>
                        <field name="order_name"/>
                    </group>
                    <group>



                    </group>
                    <footer>
                        <button name="print_report" string="Print" type="object" class="btn-primary" data-hotkey="q"/>
                        <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z" />
                    </footer>
                </form>
            </field>
        </record>

</odoo>